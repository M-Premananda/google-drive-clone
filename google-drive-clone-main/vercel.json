{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "^/(.*)\\.(js|css|png|jpg|svg|json|ico|txt|woff2)$",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
